HeaderTitleElement {
    border-color: aqua;
}

ListElement {
    border-width: 0;
    border-radius: 0;
    height: 24;
    margin: 0;

    -unity-text-align: middle-left;
    background-color: rgba(255, 255, 255, 0.0);
}

ListElement:hover {
    background-color: rgba(72, 130, 255, 0.4);
}

ListElement:active{
    background-color: rgba(72, 130, 255, 0.25);
}