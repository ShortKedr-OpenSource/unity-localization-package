<ui:UXML xmlns:ui="UnityEngine.UIElements" xmlns:uie="UnityEditor.UIElements" xsi="http://www.w3.org/2001/XMLSchema-instance" engine="UnityEngine.UIElements" editor="UnityEditor.UIElements" noNamespaceSchemaLocation="../../../../../../../UIElementsSchema/UIElements.xsd" editor-extension-mode="True">
    <Style src="LocalizationEditor.uss" />
    <uie:Toolbar name="MainToolbar" style="height: 24px;">
        <uie:ToolbarButton text="&lt;" display-tooltip-when-elided="true" style="width: 24px; -unity-text-align: middle-center; -unity-font-style: bold;" />
    </uie:Toolbar>
    <ui:VisualElement name="ContentRoot" style="flex-grow: 1; flex-direction: row;">
        <ui:VisualElement name="LocaleTab" style="min-width: 300px; max-width: 300px; flex-grow: 1; width: 30%;">
            <ui:VisualElement name="BaseLocaleView" class="unity-box" style="margin-bottom: 4px; margin-top: 4px; margin-right: 4px; margin-left: 4px; min-height: 60px; padding-bottom: 0; padding-top: 0; padding-right: 0; padding-left: 0; flex-grow: 1;">
                <uie:Toolbar>
                    <ui:Label text="Locales" display-tooltip-when-elided="true" tooltip="All locales" style="flex-grow: 1; -unity-font-style: bold; -unity-text-align: middle-center;" />
                </uie:Toolbar>
                <ui:ScrollView scroll-deceleration-rate="0,135" elasticity="0,1" show-vertical-scroller="true" class="unity-box" style="flex-grow: 1;" />
            </ui:VisualElement>
        </ui:VisualElement>
    </ui:VisualElement>
</ui:UXML>
